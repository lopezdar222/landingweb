<%if (message === 'ok') {%>
    <h2 class="text-lg font-bold md:text-xl col-span-3 md:col-span-2 flex justify-center items-center text-center"><%= title %> <%= datos.cliente_usuario %></h2>
    <!--span class="ml-2"><%= title %> <%= datos.cliente_usuario %></span-->
    <div class="w-full">
        <button onclick="cargarContenido('/principal?id_cliente=<%= datos.id_cliente %>&id_token=<%= id_token %>');" class="rounded-lg text-xs md:text-sm font-bold flex justify-center items-center disabled:opacity-40 transition py-4 relative	bg-custom-violet hover:text-gray-300 border border-custom-violet hover:border-custom-yellow w-full mb-3">
            <span class="ml-2"><<-- Volver</span>
        </button> 
        <div class="mb-4 text-center text-sm">
            <p class="text-custom-yellow font-bold">IMPORTANTE:</p>
            <div class="my-2 flex items-center justify-center">
                <span class="text-lg md:text-xl font-bold">El mínimo de Retiro es de $ <%= datos.minimo_retiro %> </span>
            </div>
            <%if (horas_ultimo_retiro < datos.minimo_espera_retiro) {%>
                <p class="text-custom-yellow font-bold">*************************************</p>
                <p class="mt-2 text-xs md:text-sm leading-5">El mínimo de espera de retiro son <%= datos.minimo_espera_retiro %> horas</p>
                <p class="mt-2 text-xs md:text-sm leading-5">Último retiro registrado hace <%= horas_ultimo_retiro %> horas.</p>
                <p class="mt-2 text-xs md:text-sm leading-5">Se podrá realizar el retiro al paso de <%= (datos.minimo_espera_retiro - horas_ultimo_retiro) %> horas.</p>
                <p class="text-custom-yellow font-bold">*************************************</p>
            <%} else {%>
            <p class="mt-2 text-xs md:text-sm leading-5">Para realizar el retiro de fichas ingresar CBU, titular e importe:</p>
            <%} %>
        </div>
        <%if (horas_ultimo_retiro >= datos.minimo_espera_retiro) {%>
            <div class="flex items-center justify-between mb-4">
                <label class="font-bold text-xs md:text-sm">CBU:</label>
                    <div class="w-2/5">
                        <div class="relative">
                            <input id="cbu" class="text-xs h-full md:text-sm rounded-lg bg-custom-dark-violet-2 focus:outline focus:outline-custom-yellow block w-full px-4 py-2 md:py-3 undefined" type="text" placeholder="XXXXXXXXXXXXXXXXXXXXXX">
                        </div>
                    </div>
                </div>
            </div>
            <div class="flex items-center justify-between mb-4">
                <label class="font-bold text-xs md:text-sm">TITULAR:</label>
                    <div class="w-2/5">
                        <div class="relative">
                            <input id="titular" class="text-xs h-full md:text-sm rounded-lg bg-custom-dark-violet-2 focus:outline focus:outline-custom-yellow block w-full px-4 py-2 md:py-3 undefined" type="text" placeholder="Titular">
                        </div>
                    </div>
                </div>
            </div>
            <div class="flex items-center justify-between mb-4">
                <label class="font-bold text-xs md:text-sm">IMPORTE:</label>
                    <div class="w-2/5">
                        <div class="relative">
                            <input id="importe" class="text-xs h-full md:text-sm rounded-lg bg-custom-dark-violet-2 focus:outline focus:outline-custom-yellow block w-full px-4 py-2 md:py-3 undefined" type="text" placeholder="10000">
                        </div>
                    </div>
                </div>
            </div>
            <span id="msgUsuario" class="ml-2"></span>
            <button onclick="retirarFichas(<%=datos.id_cliente%>, '<%=id_token%>', <%=datos.minimo_retiro%>);" class="rounded-lg text-xs md:text-sm font-bold flex justify-center items-center disabled:opacity-40 transition py-4 relative	bg-custom-violet hover:text-gray-300 border border-custom-violet hover:border-custom-yellow w-full mb-3">
                <span class="ml-2">Enviar Solicitud</span>
            </button>
        <%} %>
        <%if (datos.contacto_whatsapp != '') {%>
        <button class="rounded-lg text-xs md:text-sm font-bold flex justify-center items-center disabled:opacity-40 transition py-4 relative	bg-custom-violet hover:text-gray-300 border border-custom-violet hover:border-custom-yellow w-full mb-3">
            <a href="https://wa.me/<%= datos.contacto_whatsapp %>" target="_blank">
                <span class="ml-2">Contacto Whatsapp</span>
            </a>
        </button>
        <%} %>
        <%if (datos.contacto_telegram != '') {%>
        <button class="rounded-lg text-xs md:text-sm font-bold flex justify-center items-center disabled:opacity-40 transition py-4 relative	bg-custom-violet hover:text-gray-300 border border-custom-violet hover:border-custom-yellow w-full mb-3">
            <a href="https://<%= datos.contacto_telegram %>" target="_blank">
                <span class="ml-2">Contacto Telegram</span>
            </a>
        </button>
        <%} %>
        <button onclick="cerrar_Sesion(<%= datos.id_cliente %>, '<%= id_token %>');" class="rounded-lg text-xs md:text-sm font-bold flex justify-center items-center disabled:opacity-40 transition py-4 relative bg-custom-violet hover:text-gray-300 border border-custom-violet hover:border-custom-yellow w-full mb-3">
            <span class="ml-2">Cerrar Sesión</span>
        </button>
    </div>
    <%} else { %>
        <span class="ml-2"><%= message %> </span>
        <div class="w-full">
            <button onclick="cerrar_Sesion_Token_Invalido()" class="rounded-lg text-xs md:text-sm font-bold flex justify-center items-center disabled:opacity-40 transition py-4 relative bg-custom-violet hover:text-gray-300 border border-custom-violet hover:border-custom-yellow w-full mb-3">
                <span class="ml-2"><<-- Volver al Inicio</span>
            </button> 
        </div>    
    <%}  %>